{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently -r \"npx next\" \"npx graphql-codegen --watch\"",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json pages/api/graphql",
    "seed-db": "node ./prisma/seed.js"
  },
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "@apollo/react-hooks": "^4.0.0",
    "@graphql-tools/load": "^6.2.8",
    "@graphql-tools/schema": "^7.1.5",
    "@graphql-tools/stitch": "^7.5.3",
    "@graphql-tools/url-loader": "^6.10.1",
    "@nexus/schema": "^0.20.1",
    "@prisma/client": "^2.23.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-server-micro": "^2.25.0",
    "bcryptjs": "^2.4.3",
    "cross-fetch": "^3.1.4",
    "graphql": "^15.5.0",
    "graphql-middleware": "^6.0.10",
    "graphql-tag": "^2.12.4",
    "jsonwebtoken": "^8.5.1",
    "next": "latest",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.35.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^12.12.21",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "concurrently": "^6.2.0",
    "prisma": "^2.23.0",
    "ts-node": "^10.0.0",
    "typescript": "4.0"
  },
  "license": "MIT"
}
